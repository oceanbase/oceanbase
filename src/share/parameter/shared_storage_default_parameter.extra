[
  {
    "scenario": "express_oltp",
    "comment" : "for workloads like trade, payment core system, internet high throughput application, etc. no restrictions like foreign key, no stored procedure, no long transaction, no large transaction, no complex join, no complex subquery",
    "parameters": {
      "cluster": [
        {
          "name": "_enable_obdal",
          "value": true,
          "comment": "Enable or disable use obdal to access object storage."
        }
      ],
      "tenant": [
        {
          "name": "_tempstore_read_alignment_size",
          "value": "1MB",
          "comment": "The size of data read by tempstore each time is 1MB in shared storage mode."
        },
        {
          "name": "global_index_auto_split_policy",
          "value": "OFF",
          "comment": "Temporarily disable the automatic partitioning and splitting of the global index. Enable it again after the stability is tested"
        },
        {
          "name": "inc_sstable_upload_thread_score",
          "value": 30,
          "comment": "The number of upload threads needs to be increased. This value needs to be explored and is tentatively set at 30"
        },
        {
          "name": "_enable_active_txn_transfer",
          "value": false,
          "comment": "Temporarily disable transfer to avoid killing transactions. Reopen it after testing for stability"
        },
        {
          "name": "_io_read_redundant_limit_percentage",
          "value": 50,
          "comment": "Maximum percentage of redundant size in one read io request, redundant data means blocks in the middle of the batch that hit in cache or filtered by skipping index but must be read"
        },
        {
          "name": "_io_read_batch_size",
          "value": "128KB",
          "comment": "Maximum batch size in one read io request"
        },
        {
          "name": "log_storage_compress_all",
          "value": 1,
          "comment": "specifies whether to compress logs before storing"
        }
      ]
    }
  },
