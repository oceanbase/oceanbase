ST_ASTEXT(CAST(ST_GEOMFROMTEXT('MULTILINESTRING((1 1, 4 1, 4 4, 1 4, 1 1), (5 5, 5 6, 6 6, 6 5, 5 5))') AS POLYGON))
POLYGON((1 1,4 1,4 4,1 4,1 1),(5 5,5 6,6 6,6 5,5 5))
#
