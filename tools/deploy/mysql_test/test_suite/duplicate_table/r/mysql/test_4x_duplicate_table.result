use duptable_database;
drop table if exists tt2;
drop table if exists dup2;
CREATE TABLE `tt2` (
`x` int(11) DEFAULT NULL,
`y` int(11) DEFAULT NULL
) partition by hash(x) partitions 2;
CREATE TABLE `dup2` (
`x` int(11) DEFAULT NULL,
`y` int(11) DEFAULT NULL
) duplicate_scope='cluster';
