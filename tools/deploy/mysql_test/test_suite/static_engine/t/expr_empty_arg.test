#owner: dachuan.sdc
#owner group: sql2
#tags: optimizer

--disable_abort_on_error
--result_format 4
--explain_protocol 0

connect (conn_admin, $OBMYSQL_MS0,admin,$OBMYSQL_PWD,test,$OBMYSQL_PORT);

connection conn_admin;
--sleep 2
alter system set enable_async_syslog = false;

connection default;

--disable_warnings
drop view if exists v1;
--enable_warnings

