--echo # Merge hint
with RECURSIVE qn as (select a from t1),
     qn2 as (select b from t1)
select /*+ merge(qn) no_merge(qn2) */ qn.a,qn2.b from qn, qn2;

with RECURSIVE qn as (select a from t1)
