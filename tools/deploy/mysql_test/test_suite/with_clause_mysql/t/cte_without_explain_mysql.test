update emp2 set mgr = 5001 where ename = 'KING';
with RECURSIVE rw (ename, mgr, empno) AS
(
    SELECT ename, mgr, empno
    FROM emp2 e
    WHERE job = 'PRESIDENT'
    UNION ALL
    SELECT e.ename, e.mgr, e.empno
    FROM rw r, emp2 e
    WHERE r.empno = e.mgr and e.mgr < e.empno
)
SELECT ename, empno, mgr
FROM rw;

