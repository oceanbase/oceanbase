with RECURSIVE rw (ename, mgr, empno) AS
(
    SELECT ename, mgr, empno
    FROM emp e
    WHERE job = 'PRESIDENT'
    UNION ALL
    SELECT e.ename, e.mgr, e.empno
    FROM rw r, emp e
    WHERE r.empno = e.mgr
)
SELECT ename, empno, mgr
FROM rw;


