insert into t3 values(4, 5);
insert into t3 values(7, 8);
insert into t3 values(10, 11);
insert into t3 values(13, 14);
insert into t3 values(16, 17);

create table t4(c1 int primary key, c2 int);
CREATE INDEX i1 ON t4(c2);
