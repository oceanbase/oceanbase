from emp emp1, emp emp2, emp emp3
where emp1.empno in (
  select deptno / 10 + 7900 col1
  from dept, salgrade
  where (salgrade.grade * 5 = dept.deptno or
      salgrade.grade * 4 = dept.deptno) and
    emp2.empno > dept.deptno + 7700
  ) and
  emp1.mgr = emp2.mgr and
  emp3.mgr = emp1.mgr;
