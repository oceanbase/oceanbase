select /*+index(t1 gkey1)*/ c2, c3 from t1 order by c2 limit 3;
select /*+index(t1 gkey2)*/ c2, c3 from t1 order by c2 limit 3;

select /*+index(t1 primary)*/ * from t1 order by c2 limit 3;
